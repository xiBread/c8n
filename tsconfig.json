{
	"compilerOptions": {
		"noEmit": true,
		"outDir": "dist",
		"strict": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"module": "Preserve",
		"moduleDetection": "force",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"resolveJsonModule": true,
		"stripInternal": true,
		"types": ["vitest/importMeta"]
	}
}
